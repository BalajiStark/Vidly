@model Vidly.Models.Movie
@{
    ViewBag.Title = "MovieDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>MemberShip Type : @Model.Genre.Name</li>
    <li>Release Date: @Model.ReleaseDate.ToLongDateString()</li>
    <li>Added Date: @Model.AddedDate.ToLongDateString()</li>
    <li>Number in Stock: @Model.Stock</li>
</ul>

