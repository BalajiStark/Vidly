@model Vidly.Models.Customers
@{
    ViewBag.Title = "CustomerDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>MemberShip Type : @Model.MemberShipType.Name</li>
    @if (Model.DateofBirth.HasValue)
    {
        <li>Date of Birth: @Model.DateofBirth.Value.ToShortDateString()</li>
    }
</ul>

